###############################################
###############################################
##########        BLOCAL!            ##########
###############################################
###############################################

blocal:
      a       owl:Ontology ;
      rdfs:label "Brown Local"@en-US ;
      vitro:ontologyPrefixAnnot
              "blocal"^^xsd:string .

###############################################
##########        CLASSES!           ##########
###############################################

blocal:Delegate
      a       owl:Class ;
      rdfs:label "Delegate"@en-US ;
      rdfs:subClassOf foaf:Person ;
      vitro:displayLimitAnnot
              "-1"^^xsd:int ;
      vitro:displayRankAnnot
              "-1"^^xsd:int ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:inClassGroup brown:vitroClassGrouppeople ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> .

blocal:ClinicalDepartment
      a       owl:Class ;
      rdfs:label "Clinical Department"@en-US ;
      rdfs:subClassOf foaf:Organization ;
      vitro:descriptionAnnot
              "A type of Brown organizational unit"^^xsd:string ;
      vitro:displayLimitAnnot
              "-1"^^xsd:int ;
      vitro:displayRankAnnot
              "-1"^^xsd:int ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:inClassGroup brown:vitroClassGrouporganizations ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> .

blocal:InstituteCenterProgram
      a       owl:Class ;
      rdfs:label "Centers, Programs, and Institutes"@en-US ;
      rdfs:subClassOf foaf:Organization ;
      vitro:descriptionAnnot
              "Requested grouping for Brown local org units. "^^xsd:string ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:inClassGroup brown:vitroClassGrouporganizations ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> .

blocal:ResearchArea
      a       owl:Class ;
      rdfs:label "Research Area"@en-US ;
      rdfs:subClassOf skos:Concept ;
      vitro:descriptionAnnot
              "Subclass of skos:Concept.  At this point the distinction is for display purposes only.  "^^xsd:string ;
      vitro:displayLimitAnnot
              "-1"^^xsd:int ;
      vitro:displayRankAnnot
              "-1"^^xsd:int ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:inClassGroup brown:vitroClassGrouppublications ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:shortDef "Research topics"^^xsd:string .

blocal:CV
      a       owl:Class ;
      rdfs:label "CV"@en-US ;
      rdfs:subClassOf vivo:URLLink ;
      vitro:displayLimitAnnot
              "-1"^^xsd:int ;
      vitro:displayRankAnnot
              "-1"^^xsd:int ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:shortDef "PDF of CV"^^xsd:string .

blocal:Place
      a       owl:Class ;
      rdfs:label "Place"@en-US ;
      rdfs:subClassOf skos:Concept ;
      vitro:inClassGroup brown:vitroClassGrouplocations .

