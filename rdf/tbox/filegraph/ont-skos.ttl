@prefix d: <http://vivo.brown.edu/individual/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#> .
@prefix vitrorole: <http://vitro.mannlib.cornell.edu/ns/vitro/role#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

skos:Concept a owl:Class ;
    rdfs:label "Concept"@en-US ;
    vitro:displayLimitAnnot "-1"^^xsd:int ;
    vitro:displayRankAnnot "35"^^xsd:int ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot vitrorole:public ;
    vitro:inClassGroup d:vitroClassGrouppublications ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot vitrorole:public ;
    vitro:shortDef "An idea or notion; a unit of thought."^^xsd:string .

skos:exactMatch a rdf:Property,
        owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:label "has exact match"@en-US ;
    vitro:displayLimitAnnot "-1"^^xsd:int ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot vitrorole:dbAdmin ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot vitrorole:dbAdmin ;
    vitro:selectFromExistingAnnot true ;
    rdfs:comment "skos:exactMatch is disjoint with each of the properties skos:broadMatch and skos:relatedMatch."@en ;
    rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

<http://www.w3.org/2004/02/skos/core> a owl:Ontology ;
    rdfs:label "SKOS (Simple Knowledge Organization System)"@en-US ;
    vitro:ontologyPrefixAnnot "skos"^^xsd:string .

