###############################################
###############################################
##########          SKOS!            ##########
###############################################
###############################################

<http://www.w3.org/2004/02/skos/core>
      a       owl:Ontology ;
      rdfs:label "SKOS (Simple Knowledge Organization System)"@en-US ;
      vitro:ontologyPrefixAnnot
              "skos"^^xsd:string .


###############################################
##########        CLASSES!           ##########
###############################################

skos:Concept
      a       owl:Class ;
      rdfs:label "Concept"@en-US ;
      vitro:displayLimitAnnot
              "-1"^^xsd:int ;
      vitro:displayRankAnnot
              "35"^^xsd:int ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:inClassGroup brown:vitroClassGrouppublications ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
      vitro:shortDef "An idea or notion; a unit of thought."^^xsd:string .

###############################################
##########        PROPERTIES!        ##########
###############################################

skos:exactMatch
      a       rdf:Property , owl:TransitiveProperty , owl:ObjectProperty ;
      rdfs:comment "skos:exactMatch is disjoint with each of the properties skos:broadMatch and skos:relatedMatch."@en ;
      rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
      rdfs:label "has exact match"@en-US ;
      vitro:displayLimitAnnot
              "-1"^^xsd:int ;
      vitro:hiddenFromDisplayBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#dbAdmin> ;
      vitro:prohibitedFromUpdateBelowRoleLevelAnnot
              <http://vitro.mannlib.cornell.edu/ns/vitro/role#dbAdmin> ;
      vitro:selectFromExistingAnnot
              "true"^^xsd:boolean .