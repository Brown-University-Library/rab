@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vitroDisplay: <http://vitro.mannlib.cornell.edu/ontologies/display/1.1#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix core: <http://vivoweb.org/ontology/core#> .
@prefix blocal: <http://vivo.brown.edu/ontology/vivo-brown/> .
@prefix bdisplay: <http://vivo.brown.edu/ontology/display#> .
@prefix bprofile: <http://vivo.brown.edu/ontology/profile#> .

 vitroDisplay:SearchIndex 
    rdf:type owl:Thing ; 
    vitroDisplay:excludeClass bdisplay:Hidden ;
    vitroDisplay:excludeClass bprofile:Appointment ;
    vitroDisplay:excludeClass bprofile:Certification ;
    vitroDisplay:excludeClass bprofile:Credential ;
    vitroDisplay:excludeClass bprofile:HospitalAppointment ;
    vitroDisplay:excludeClass bprofile:License ;
    vitroDisplay:excludeClass bprofile:PostdocAppointment ;
    vitroDisplay:excludeClass core:FacultyPosition ; 
    vitroDisplay:excludeClass core:EducationalTraining ;    
    vitroDisplay:excludeClass core:DateTimeValue ;
    vitroDisplay:excludeClass core:DateTimeInterval ;
    vitroDisplay:excludeClass core:AcademicDegree ;
    vitroDisplay:excludeClass core:URLLink ;
    vitroDisplay:excludeClass blocal:Delegate ; 
    vitroDisplay:excludeClass <http://vitro.mannlib.cornell.edu/ns/vitro/public#File> ;
    vitroDisplay:excludeClass <http://vitro.mannlib.cornell.edu/ns/vitro/public#FileByteStream> .
